package model;

public class Zeitmessung 
{
	public long Zeitstempel;
	
	public void startZeit()
	{
		long Timestamp1;

		Timestamp1 = System.currentTimeMillis();
		System.out.println("Zeitmessung gestartet!");
		Zeitstempel = Timestamp1;
	}
		
	public void stopZeit()
	{
		long Timestamp2;
		
		long Timestamp1 = Zeitstempel;
		
		Timestamp2 = System.currentTimeMillis();
		System.out.println("Die Ausführung hat " + ((Timestamp2-Timestamp1))+ " Millisekunden");
	}
}
